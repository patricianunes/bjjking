<%= form_for @team do |f| %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, :autofocus => true, :required => true, :class => 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :logo %>
    <%= f.url_field :logo, :class => 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address, :class => 'form-control' %>
  </div>

  <%= f.submit%>
<% end %>
